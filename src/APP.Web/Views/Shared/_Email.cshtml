<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"]</title>
    <style>
        .container {
            padding:10px;
        }

        .header {
            min-height: 100px;
            border-bottom: 3px solid #1B4A7E;
        }

        .body {
            padding: 30px 5px;
            border-bottom: 3px solid #1B4A7E;
        }
    </style>
</head>
<body>
    <div class="container">
        
        <div class="body">
            @RenderBody()
        </div>
    </div>
</body>
</html>


